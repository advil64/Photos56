<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="440.0" prefWidth="714.0" 
	xmlns="http://javafx.com/javafx/11.0.1" 
	xmlns:fx="http://javafx.com/fxml/1" 
	fx:controller="controller.SlideShowController">
   <children>
      <ImageView fx:id="img" fitHeight="282.0" fitWidth="473.0" layoutX="128.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <ButtonBar layoutX="111.0" layoutY="313.0" prefHeight="40.0" prefWidth="492.0">
        <buttons>
          <Button mnemonicParsing="false" onMouseClicked="#prevImage" prefHeight="48.0" prefWidth="276.0" text="Prev">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onMouseClicked="#nextImage" prefHeight="47.0" text="Next">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
        </buttons>
      </ButtonBar>
      <Button layoutX="303.0" layoutY="376.0" mnemonicParsing="false" onMouseClicked="#goBack" prefHeight="40.0" prefWidth="129.0" text="Go Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
