<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="440.0" prefWidth="714.0" 
	xmlns="http://javafx.com/javafx/11.0.1" 
	xmlns:fx="http://javafx.com/fxml/1" 
	fx:controller="view.AdminController">
   <children>
      <ListView fx:id="username_list" layoutX="4.0" layoutY="2.0" prefHeight="434.0" prefWidth="255.0" />
      <Label layoutX="290.0" layoutY="156.0" text="Username:">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="username_textfield" layoutX="414.0" layoutY="160.0" prefHeight="26.0" prefWidth="200.0" />
      <ButtonBar layoutX="302.0" layoutY="206.0" prefHeight="40.0" prefWidth="278.0">
        <buttons>
            <Button fx:id="add_button" mnemonicParsing="false" onMouseClicked="#add" prefHeight="43.0" text="Add">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
          <Button fx:id="delete_button" mnemonicParsing="false" onMouseClicked="#delete" prefHeight="38.0" prefWidth="106.0" text="Delete">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
        </buttons>
      </ButtonBar>
      <Button fx:id="logout_button" layoutX="377.0" layoutY="266.0" mnemonicParsing="false" onMouseClicked="#logout" prefHeight="39.0" prefWidth="171.0" text="Logout">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
